<div id="login">
  <h3>Sign In</h3>
  <%= form_for :session, url: login_path do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit 'Log In' %>
  <% end %>

  <h4>New User?</h4>
  <%= link_to 'Sign Up!', new_user_path %>
</div>
